# API 配置
VITE_API_BASE_URL=https://api.example.com/api
VITE_API_TIMEOUT=10000
VITE_API_RETRIES=3

# 存储配置
VITE_STORAGE_VERSION=1.0
VITE_STORAGE_PREFIX=demoai:
VITE_STORAGE_TTL=300000
VITE_STORAGE_QUOTA_WARNING=80
VITE_STORAGE_QUOTA_ERROR=90

# 性能监控配置
VITE_PERFORMANCE_WARNING_THRESHOLD=1000
VITE_PERFORMANCE_ERROR_THRESHOLD=3000
VITE_PERFORMANCE_MAX_SAMPLES=100
VITE_PERFORMANCE_METRICS_RETENTION=86400000

# 特性开关
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_DATA_COMPRESSION=true
VITE_ENABLE_AUTO_SYNC=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true

# 应用信息
VITE_APP_TITLE=DemoAI
VITE_APP_DESCRIPTION=AI应用管理平台
VITE_APP_VERSION=1.0.0 