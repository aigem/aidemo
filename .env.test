# API 配置
VITE_API_BASE_URL=http://test-api.example.com/api
VITE_API_TIMEOUT=20000
VITE_API_RETRIES=5

# 存储配置
VITE_STORAGE_VERSION=test
VITE_STORAGE_PREFIX=demoai:test:
VITE_STORAGE_TTL=60000
VITE_STORAGE_QUOTA_WARNING=90
VITE_STORAGE_QUOTA_ERROR=95

# 性能监控配置
VITE_PERFORMANCE_WARNING_THRESHOLD=2000
VITE_PERFORMANCE_ERROR_THRESHOLD=5000
VITE_PERFORMANCE_MAX_SAMPLES=1000
VITE_PERFORMANCE_METRICS_RETENTION=3600000

# 特性开关
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_DATA_COMPRESSION=true
VITE_ENABLE_AUTO_SYNC=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true

# 应用信息
VITE_APP_TITLE=DemoAI (Test)
VITE_APP_DESCRIPTION=AI应用管理平台 (测试版)
VITE_APP_VERSION=1.0.0-test 